{
  "expo": {
    "name": "Cetis-27",
    "slug": "Cetis-27",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoCetis.png",
    "scheme": "cetis27",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "Esta app necesita acceso a Bluetooth para registrar asistencias mediante dispositivos Bluetooth.",
        "NSBluetoothPeripheralUsageDescription": "Esta app necesita acceso a Bluetooth para conectarse con dispositivos cercanos.",
        "NSLocationWhenInUseUsageDescription": "Esta app necesita acceso a la ubicaci√≥n para escanear dispositivos Bluetooth cercanos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/logoCetis.png",
        "monochromeImage": "./assets/images/logoCetis.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_ADVERTISE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_ADVERTISE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.luis_03.Cetis27"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logoCetis.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoCetis.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-permissions",
        {
          "permissions": [
            "BLUETOOTH",
            "BLUETOOTH_ADMIN",
            "BLUETOOTH_SCAN",
            "BLUETOOTH_CONNECT",
            "BLUETOOTH_ADVERTISE",
            "ACCESS_FINE_LOCATION",
            "ACCESS_COARSE_LOCATION"
          ],
          "iosPermissions": ["BLUETOOTH", "LOCATION_WHEN_IN_USE"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0e37a8e-5a7b-423c-9bf6-c7210cfdf3ea"
      }
    }
  }
}
